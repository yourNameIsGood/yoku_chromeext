{
  "manifest_version": 2,

  "name": "NXG Data Graph",
  "description": "Get to know Data Graph Better",
  "version": "1.0",

  "browser_action": {
    "default_icon": "static/icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches":["*://*/*"],
      "js": ["/static/jquery.min.js","/static/content_script.js"]
    }
  ],
  "permissions": [ 
        "cookies",
        "tabs",
        "activeTab",
        "http://*/*",
        "https://*/*"
  ]
}
